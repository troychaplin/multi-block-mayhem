{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "multi-block-mayhem/image-collage-image",
	"version": "0.1.0",
	"title": "Image Collage: Single Image",
	"category": "media",
	"description": "A single image block that supports the primary image collage.",
	"parent": ["multi-block-mayhem/image-collage"],
	"usesContext": [
		"multi-block-mayhem/image-collage-columns",
		"multi-block-mayhem/image-collage-aspect-ratio"
	],
	"supports": {
		"align": ["full", "wide"],
		"alignWide": true,
		"html": false
	},
	"attributes": {
		"columnSpan": {
			"type": "number",
			"default": 1
		},
		"columns": {
			"type": "number",
			"default": 3
		},
		"aspectRatio": {
			"type": "string",
			"default": "4/3"
		},
        "imageId": {
            "type": "number",
            "default": null
        },
		"imageUrl": {
			"type": "string",
			"default": null
		},
		"imageResolution": {
			"type": "string",
			"default": "large"
		},
		"imageWidth": {
			"type": "number",
			"default": null
		},
		"imageHeight": {
			"type": "number",
			"default": null
		},
		"focalPoint": {
			"type": "object",
			"default": {
				"x": 0.5,
				"y": 0.5
			}
		},
		"zoom": {
			"type": "number",
			"default": 0
		}
	},
	"textdomain": "multi-block-mayhem",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
